{
    "manifest_version": 3,
    "name": "Extensions ALFA",
    "icons":{
        "512":"images/alfa.png"
    },
    "version": "1.0.0",
    "action":{
      "default_popup":"index.html"
    },
    "permissions": [
      "scripting",
      "storage",
      "activeTab"
    ],
    
    "host_permissions":[
      "http://*/*",
      "https://*/*"
    ], 
    "web_accessible_resources": [{
      "resources": ["images/close.png"],
      "matches": ["<all_urls>"]
    }],
    "content_scripts": [
      {
        "js": ["bundle-ext.js"],
        "matches":[
            "http://*/*",
            "https://*/*"
        ],
        "css": ["content.css"]
      }
    ],
    "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
  }