{
    "manifest_version": 3,
    "name": "Extensions ALFA",
    "icons":{
        "256":"plus_a.png"
    },
    "version": "1.0.0",
    "action":{
      "default_popup":"index.html"
    },
    "permissions": [
      "scripting",
      "storage",
      "activeTab"
    ],
    
    "host_permissions":[
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "js": ["ext-bundle.js"],
        "matches":[
            "http://*/*",
            "https://*/*"
        ]
      }
    ],
    "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
  }